@model Lab3.Models.Student
@{
    Layout = "_Layout";
}
<table>
    <tr>
        <td>First Name:</td>
        <td>@Model.firstName</td>
    </tr>
    <tr>
        <td>Last Name:</td>
        <td>@Model.lastName</td>
    </tr>
    <tr>
        <td>Student ID:</td>
        <td>@Model.studentID</td>
    </tr>
        <tr>
        <td>Email Address:</td>
        <td>@Model.email</td>
    </tr>
    <tr>
        <td>Description of Student:</td>
        <td>@Model.description</td>
    </tr>



</table>